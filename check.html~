<SCRIPT LANGUAGE="JavaScript">
function LogIn(){
loggedin=false;
username="";
password="";
username=prompt("Username:","");
username=username.toLowerCase();
password=prompt("Password:","");
password=password.toLowerCase();
if (username=="abfriend" && password=="abhishek") { 
loggedin=true;
window.location="resume.html";
}
if (loggedin==false) {
alert("Invalid login!");
}
}
</SCRIPT> 
